<PROCESSORS>
   <!-- Processor ISA is in Hthreads naming convention ('mblaze' as opposed to 'microblaze') -->
   <PROCESSOR HTHREADS_ISA="mblaze" HWVERSION="9.5">
      <DOCUMENT SOURCE="http://www.xilinx.com/support/documentation/sw_manuals/xilinx2015_1/ug984-vivado-microblaze-ref.pdf"/>
      <PARAMETER NAME="C_USE_DIV" VALUE="1" OPCODE="010010" STRUCT_ENTRY="idiv">
         <OPTION VALUE="idiv"/>
         <OPTION VALUE="idivu"/>
      </PARAMETER>
      <PARAMETER NAME="C_USE_FPU" VALUE="1" OPCODE="010110" STRUCT_ENTRY="fpu">
         <OPTION VALUE="fadd"/>
         <OPTION VALUE="frsub"/>
         <OPTION VALUE="fsub"/>
         <OPTION VALUE="fmul"/>
         <OPTION VALUE="fdiv"/>
         <OPTION VALUE="fcmp.lt"/>
         <OPTION VALUE="fcmp.eq"/>
         <OPTION VALUE="fcmp.le"/>
         <OPTION VALUE="fcmp.gt"/>
         <OPTION VALUE="fcmp.ne"/>
         <OPTION VALUE="fcmp.ge"/>
         <OPTION VALUE="fcmp.un"/>
         <!-- C_USE_FPU = 2 -->
         <OPTION VALUE="fsqrt"/>
         <OPTION VALUE="flt"/>
         <OPTION VALUE="fint"/>
      </PARAMETER>
      <PARAMETER NAME="C_USE_BARREL" VALUE="1" OPCODE="010001" STRUCT_ENTRY="barrel">
         <OPTION VALUE="bsrl"/>
         <OPTION VALUE="bsra"/>
         <OPTION VALUE="bsll"/>
      </PARAMETER>
      <PARAMETER NAME="C_USE_HW_MUL" VALUE="1" OPCODE="010000" STRUCT_ENTRY="mul">
         <OPTION VALUE="mul"/>
         <!-- C_USE_HW_MUL = 2 -->
         <OPTION VALUE="mulh"/>
         <OPTION VALUE="mulhu"/>
         <OPTION VALUE="mulhsu"/>
         <OPTION VALUE="muli"/>
      </PARAMETER>
   </PROCESSOR>

</PROCESSORS>
